{
    "name": "gusvasconcelos/markdown-converter",
    "description": "A PHP library for generating structured content in Markdown format with support for various elements and customization options",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "GusVasconcelos\\MarkdownConverter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\GusVasconcelos\\MarkdownConverter\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Gustavo Vasconcelos",
            "email": "gusvasconcelos@proton.me"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "phpunit"
    }
}
